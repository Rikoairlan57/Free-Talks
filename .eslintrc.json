{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "quotes": "off",
    "no-console": "off",
    "import/order": "off",
    "consistent-return": "off",
    "prefer-destructuring": "off",
    "import/newline-after-import": "off",
    "no-global-assign": "off",
    "import/no-useless-path-segments": "off",
    "import/no-self-import": "off"
  }
}
